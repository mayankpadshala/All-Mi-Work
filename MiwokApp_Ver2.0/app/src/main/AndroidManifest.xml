<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.android.miwokapp_2">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.example.android.miwokapp_2.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.android.miwokapp_2.NumbersActivity"
            android:label="Numbers"
            android:parentActivityName="com.example.android.miwokapp_2.MainActivity"/>

        <activity
            android:name="com.example.android.miwokapp_2.FamilyMembersActivity"
            android:label="Family Members"
            android:parentActivityName="com.example.android.miwokapp_2.MainActivity"/>


        <activity
            android:name="com.example.android.miwokapp_2.ColorsActivity"
            android:label="Colors"
            android:parentActivityName="com.example.android.miwokapp_2.MainActivity"/>

        <activity
            android:name="com.example.android.miwokapp_2.PhrasesActivity"
            android:label="Pharses"
            android:parentActivityName="com.example.android.miwokapp_2.MainActivity"/>
    </application>

</manifest>